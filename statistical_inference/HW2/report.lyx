\begin_layout Subsection*
b)
\end_layout

\begin_layout Standard
If two events (A,B) are disjointed such that P(A)=1 and P(B)=0 then:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P(A\cap B)=0
\]

\end_inset

and 
\begin_inset Formula 
\[
P(B|A)=\frac{P(A\cap B)=0}{P(A)}=0=P(B)
\]

\end_inset


\end_layout

